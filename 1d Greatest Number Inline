#include <iostream>
using namespace std;

inline float compute(float x, float y, float z)
{
	if(x>y && x >z)
		return x;
	else if(y>x && y>z)
		return y;
	else
		return z;
}


int main()
{
	float x,y,z;

	cout<<"Enter three numbers: "<<endl;
	cin>> x >> y >> z;
	cout<<"The greatest number is: " << compute(x,y,z)<<endl;

}
